      "comma-dangle": ["error", "always"]
    }
  },
  "devDependencies": {
    "eslint": "^2.8.0",
    "javascript-styleguide": "^0.2.0",
    "nib": "~1.1.0",
    "stylus": "~0.54.2",
    "uglify-js": "~2.6.2"
  }
}
